{
  "name": "musik-cube",
  "version": "1.0.0",
  "description": "A musical Rubik's Cube, using arduino, rasbpi, and sonic-pi",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts --fix",
    "prepublish": "tsc",
    "start:http": "ts-node ./src/interfaces/http",
    "start:random": "ts-node ./src/interfaces/random",
    "start:serial": "ts-node ./src/interfaces/serial",
    "start:udp": "ts-node ./src/interfaces/udp",
    "test": "tsc && jest ./lib"
  },
  "author": "Peter Hayes",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.3",
    "@types/express": "^4.17.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-peterkhayes": "^1.0.1",
    "jest": "^24.9.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
